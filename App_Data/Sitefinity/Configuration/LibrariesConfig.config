		<add title="Quantum Sweets Libraries" applicationName="librariesProvider5" urlName="librariesprovider5" type="Telerik.Sitefinity.Modules.Libraries.Data.OpenAccessLibrariesProvider, Telerik.Sitefinity" enabled="False" name="librariesProvider5" />
		<add title="Quantum Sweets Libraries" applicationName="librariesProvider6" urlName="librariesprovider6" type="Telerik.Sitefinity.Modules.Libraries.Data.OpenAccessLibrariesProvider, Telerik.Sitefinity" enabled="False" name="librariesProvider6" />
		<add title="Quantum Fashion Libraries" applicationName="librariesProvider8" urlName="librariesprovider8" type="Telerik.Sitefinity.Modules.Libraries.Data.OpenAccessLibrariesProvider, Telerik.Sitefinity" enabled="False" name="librariesProvider8" />
		<add title="Quantum Fashion Libraries" applicationName="librariesProvider9" urlName="librariesprovider9" type="Telerik.Sitefinity.Modules.Libraries.Data.OpenAccessLibrariesProvider, Telerik.Sitefinity" enabled="False" name="librariesProvider9" />
		<add title="Fashion Libraries" applicationName="librariesProvider10" urlName="librariesprovider10" type="Telerik.Sitefinity.Modules.Libraries.Data.OpenAccessLibrariesProvider, Telerik.Sitefinity" enabled="False" name="librariesProvider10" />
		<add title="Quantum Fashion Libraries" applicationName="librariesProvider11" urlName="librariesprovider11" type="Telerik.Sitefinity.Modules.Libraries.Data.OpenAccessLibrariesProvider, Telerik.Sitefinity" enabled="False" name="librariesProvider11" />
		<add title="Quantum Fashion Libraries" applicationName="librariesProvider12" urlName="librariesprovider12" type="Telerik.Sitefinity.Modules.Libraries.Data.OpenAccessLibrariesProvider, Telerik.Sitefinity" enabled="False" name="librariesProvider12" />
	</providers>
</librariesConfig>