    "Theme": null,
    "DataType": "ASP_NET_TEMPLATE",
    "Data": "@model Telerik.Sitefinity.Frontend.Mvc.Models.ContentDetailsViewModel\r\n\r\n@using System.Collections;\r\n@using System.Linq;\r\n@using Telerik.Sitefinity.Frontend.DynamicContent.WidgetTemplates.Fields.Helpers;\r\n@using Telerik.Sitefinity;\r\n@using Telerik.Sitefinity.Data.ContentLinks;\r\n@using Telerik.Sitefinity.Frontend.Mvc.Helpers;\r\n@using Telerik.Sitefinity.Libraries.Model;\r\n@using Telerik.Sitefinity.Localization;\r\n@using Telerik.Sitefinity.Model;\r\n@using Telerik.Sitefinity.Model.ContentLinks;\r\n@using Telerik.Sitefinity.Modules.Pages;\r\n@using Telerik.Sitefinity.Pages.Model; \r\n\r\n<div class=\"@Model.CssClass\" @Html.InlineEditingAttributes(Model.ProviderName, Model.ContentType.FullName, (Guid)Model.Item.Fields.Id)>\r\n    <h3 @Html.InlineEditingFieldAttributes(\"TestimonialAuthor\", \"ShortText\")>\r\n        @Model.Item.Fields.TestimonialAuthor\r\n    </h3>\r\n    <div>\r\n        @Model.Item.GetDateTime(\"PublicationDate\", \"MMM d, yyyy, HH:mm tt\")\r\n        @Html.CommentsCount(\"\", @Model.Item.DataItem)\r\n    </div>\r\n    \r\n    \r\n@*Start Quote field*@\r\n<div @Html.InlineEditingFieldAttributes(\"Quote\", \"LongText\")>@Html.Raw(Model.Item.Fields.Quote)</div>\r\n@*End Quote field*@\r\n\r\n@*Start JobTitle field*@\r\n<div>\r\n    <strong> Job title :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"JobTitle\", \"ShortText\")>@Model.Item.Fields.JobTitle</span>    \r\n</div>\r\n@*End JobTitle field*@\r\n\r\n@*Start Company field*@\r\n<div>\r\n    <strong> Company :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"Company\", \"ShortText\")>@Model.Item.Fields.Company</span>    \r\n</div>\r\n@*End Company field*@\r\n\r\n@*Start Photo field*@\r\n@if(Model.Item.Fields.Photo != null)\r\n{\r\n    <img src='@Model.Item.Fields.Photo.Fields.MediaUrl' alt='@Model.Item.Fields.Photo.Fields.AlternativeText' title='@Model.Item.Fields.Photo.Fields.Title' />\r\n}\r\n@*End Photo field*@\r\n\r\n    \r\n    @if (Model.EnableSocialSharing)\r\n    {\r\n        @Html.SocialShareOptions()\r\n    }\r\n\r\n    @Html.CommentsList(@Model.Item.DataItem)\r\n</div>",
